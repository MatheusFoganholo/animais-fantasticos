{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-param-reassign": 0,
      "import/extensions": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0
    }
}
